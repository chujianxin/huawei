<script type="text/javascript" src="/marketplace/portal/static/isv/js/minions/minions.js?v=b5da1451-9ff3-4ee2-9d3b-a8bcdab0c860"></script><script>
  
jQuery(function(){	
    var tmp_callback = 'jQuery' + 'DGjf4bCw';
    //判断登录
    var login_check_url = "https://10.175.38.41/marketplace/portal/index/islogin?callback=?";
    window.raw_onerror = window.onerror;
    window.onerror = function(errorMsg, url, lineNumber) {
        return true;
    }
    var head = document.getElementsByTagName('head')[0];
    var script = document.createElement('script');
    script.src = login_check_url.replace('callback=?', 'callback=' + tmp_callback);
    head.appendChild(script);
    script.onload = function() {
        window.onerror = window.raw_onerror;
    }

    window[tmp_callback] = function(data) {
        if (data.flag == 1) {
        	jQuery(".headerAd .ht-login").remove();
        	jQuery(".headerAd .ht-fl").remove();
        	jQuery(".headerAd .ht-reg").remove();
        	changeLoginStatus(data.username); 
        	changeIsvStatus(data.isvStatus);    	     
        }
        delete(window[tmp_callback]);
    }    
});
$("body [mate_data_ts]").each(function() {
    $(this).bind('click', function() {
        var ge_get = $(this).attr('mate_data_ts');
        var ge_gets = ge_get.split('.');
        if(ge_gets.length){
        	sendGA('send', 'event', ge_gets[0]||'', ge_gets[1]||'', ge_gets[2]||'', +ge_gets[3]||1);
        }
    });
});
</script>
 
